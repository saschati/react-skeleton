{
  "name": "react-skeleton",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "formik": "^2.4.5",
    "google-libphonenumber": "^3.2.33",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "mime": "^3.0.0",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-paginate": "^8.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.19.0",
    "sass": "^1.69.5",
    "the-new-css-reset": "^1.11.2",
    "vite-plugin-svgr": "^4.2.0",
    "web-vitals": "^3.5.0",
    "yup": "^1.3.2"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "analyze": "BOUNDLE_ANALYZE=true tsc && vite build",
    "eslint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "eslint:fix": "eslint --fix src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "stylelint": "stylelint \"src/**/*.scss\" --custom-syntax postcss-scss",
    "prettier": "prettier --write \"**/*.+(js|ts|jsx|tsx|json|css|scss|md)\"",
    "storybook": "storybook dev -h 0.0.0.0 -p 80",
    "build-storybook": "storybook build",
    "postinstall": "patch-package"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.202",
    "@types/mime": "^3.0.4",
    "@types/node": "^20.9.3",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "patch-package": "^8.0.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.1.0",
    "storybook": "^7.5.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-prettier": "^4.0.2",
    "stylelint-selector-bem-pattern": "^3.0.1",
    "typescript": "5.2.2",
    "vite": "^5.0.1"
  },
  "packageManager": "yarn@4.0.1"
}
