{
  "name": "react-skeleton",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "classnames": "^2.3.2",
    "formik": "^2.2.9",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-paginate": "^8.1.4",
    "react-router-dom": "^6.7.0",
    "sass": "^1.57.1",
    "web-vitals": "^3.3.0",
    "yup": "^1.0.2"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "analyze": "BOUNDLE_ANALYZE=true tsc && vite build",
    "eslint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "eslint:fix": "eslint --fix src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "stylelint": "stylelint \"src/**/*.scss\" --custom-syntax postcss-scss",
    "prettier": "prettier --write \"**/*.+(js|ts|jsx|tsx|json|css|scss|md)\"",
    "postinstall": "patch-package"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "patch-package": "^6.5.1",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.3",
    "stylelint": "^15.3.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-prettier": "^3.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.0"
  }
}
